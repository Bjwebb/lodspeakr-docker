prefix rp: <http://resourceprojects.org/def/>
prefix rp_misc: <http://resourceprojects.org/def/misc/>
prefix skos_: <http://www.w3.org/2004/02/skos/core#>

SELECT ?project ?name ?country_name ?commodityType WHERE {
    <{{uri}}> rp:groupMember ?members .
    ?members rp:organisation ?company .
    ?company rp:hasStake ?stakes .
    ?stakes rp:isStakeIn ?project .
    ?project a rp:Project  .
    ?project skos_:prefLabel ?name .
    ?project rp:hasLocation ?country .
    ?country skos_:prefLabel ?country_name 
    OPTIONAL { ?participant rp_misc:share ?share }
    OPTIONAL { ?project rp:commodity ?commodity .
              ?commodity rp_misc:commodityType ?commodityType }

}
GROUP BY ?project ?name
ORDER BY ?name
