prefix rp: <http://resourceprojects.org/def/>
prefix skos_: <http://www.w3.org/2004/02/skos/core#>

SELECT * WHERE {
    ?participant rp:hasLocation <{{uri}}> .
    ?participant a rp:Participant .
    ?participant rp:organization ?company .
    OPTIONAL {
        ?company skos_:prefLabel ?name
    }
    FILTER NOT EXISTS {
        ?participant owl:sameAs ?b
    }
} LIMIT {{lodspk.maxResults}}
